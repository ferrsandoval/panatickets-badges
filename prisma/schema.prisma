generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PrintJob {
  id          String    @id @default(cuid())
  scanId      String?   @unique @map("scan_id")
  contentHash String?   @unique @map("content_hash")
  name        String
  rawPayload  String?   @map("raw_payload")
  printedAt   DateTime? @map("printed_at")
  createdAt   DateTime  @default(now()) @map("created_at")

  @@index([printedAt, createdAt])
  @@map("print_jobs")
}
